{
  "name": "roots",
  "version": "0.1.0",
  "proxy": "http://localhost:5000",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "axios-debug-log": "^1.0.0",
    "concat": "^1.0.3",
    "nanoid": "^4.0.2",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-ui-avatars": "^3.0.2",
    "styled-components": "^6.0.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "postcss": "^8.4.26",
    "postcss-cli": "^10.1.0",
    "react-router-dom": "^6.14.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass src/sass/main.scss src/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/style.comp.css",
    "concat:css": "concat -o src/style.concat.css src/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/style.concat.css -o src/style.prefix.css",
    "compress:css": "node-sass src/style.prefix.css src/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
